Year = int(input(“Enter the year you want to see if it was a leap year or not”))
LeapYear = FALSE
if (mod(Year, 4) == 0) then
	LeapYear = TRUE
endif
if (mod(Year,100) == 0) then 
	LeapYear = FALSE
endif
if (mod (Year,400) == 0) then
	LeapYear = TRUE
Endif
If LeapYear = TRUE then
	Print(Year, “is not a Leap Year”)
endif
