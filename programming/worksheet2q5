trigger = str(input())
if trigger == "on":
    if movementGround == true or movementFirst == true:
        Alarm = on
	If movementGround == true and movementFirst == true:
		Print(“Intruder Alert on both floors”)
        elif movementGround == true:
		print("Intruder Alert on ground floor")
	else:
		print("Intruder Alert on first floor")
